<p id="notice"><%= notice %></p>

<h1>Listing Catering Orders</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @catering_orders.each do |catering_order| %>
      <tr>
        <td><%= link_to 'Show', catering_order %></td>
        <td><%= link_to 'Edit', edit_catering_order_path(catering_order) %></td>
        <td><%= link_to 'Destroy', catering_order, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Catering order', new_catering_order_path %>
